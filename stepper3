#include <StepperMulti.h>

StepperMulti stepper1(200, 2, 4, 3, 5);
StepperMulti stepper2(200, 6, 8, 7, 9);
 
 
void setup()
{
  Serial.begin(9600);
  stepper1.setSpeed(60);
  stepper2.setSpeed(60);
}
 
void loop()
{
  if (Serial.available())
  {
    Serial.read();
    stepper1.setStep(200);
    stepper2.setStep(200);
  }
  
  stepper1.moveStep();
  stepper2.moveStep();
}
